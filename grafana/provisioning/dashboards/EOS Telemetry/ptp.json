        {
          "dataset": "iox",
          "datasource": {
            "type": "influxdb",
            "uid": "PF14EF51A41728637"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH \n-- 1. Build the Grandmaster Clock Identity as a EUI-64 string\nformatted_data AS (\n  SELECT\n    time,\n    source,\n    concat(\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v0\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v1\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v2\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v3\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v4\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v5\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v6\"), 2, '0'), ':',\n        lpad(to_hex(\"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v7\"), 2, '0')\n    ) AS gm_id\n  FROM \"ptp\"\n  WHERE\n    source IN (${device:singlequote}) -- Only get the devices filtered by the dashboard\n    AND \"Sysdb/ptp/status/parentDS/grandmasterIdentity/value/v0\" IS NOT NULL -- Check if the first octet is not null\n),\n-- The values at \"Sysdb/ptp/status/parentDS/grandmasterIdentity/value\" only change if there is a GM change or if PTP Agent starts\n-- 2. We need to get the latest gm_id BEFORE the dashboard beginning\ninitial_state AS (\n  SELECT\n    -- TRICK : We force time to be the dashboard beginning\n    CAST($__timeFrom AS TIMESTAMP) as time, \n    source,\n    gm_id,\n    -- We order to only keep the most recent values of the past\n    ROW_NUMBER() OVER (PARTITION BY source ORDER BY time DESC) as rn\n  FROM formatted_data\n  WHERE time < $__timeFrom -- We look in the past (before the dashboard time range)\n),\n\n-- 3. We query the data in the dashboard time range\ncurrent_changes AS (\n  SELECT\n    time,\n    source,\n    gm_id\n  FROM formatted_data\n  WHERE time >= $__timeFrom AND time <= $__timeTo\n)\n\n-- 4. We combine both (UNION)\nSELECT time, source, gm_id \nFROM initial_state \nWHERE rn = 1 -- We only keep the latest value of the past\nUNION ALL\nSELECT time, source, gm_id \nFROM current_changes\nORDER BY time ASC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ]
          }
        }
      ],
      "title": "Grandmaster Clock Identity",
